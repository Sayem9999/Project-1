{
  "run": [
    {
      "method": "shell.run",
      "params": {
        "id": "tailscale_funnel",
        "path": "backend",
        "message": "powershell -ExecutionPolicy Bypass -File .\\tailscale_funnel.ps1",
        "daemon": true
      }
    },
    {
      "method": "notify",
      "params": {
        "html": "<h3>Tailscale Funnel Started</h3><p>Use the tailscale_funnel shell tab to copy your public URL.</p>"
      }
    }
  ]
}
